#include<iostream>
using namespace std;
int main()
{
	int a[10],i,n;
	int *p;
	cout<<"Enter size of the array: ";
	cin>>n;
	cout<<"Enter array elements: ";
	for (i = 0; i<n; i++)
	{
		cin>>a[i];
	}
	cout<<"Enter elements:\n";
	for(i=0;i<n;i++)
	{
		cout<<"\n"<<a[i];
	}
	p = a;
	cout<<"\nYou entered:\n";
	for(i=0;i<n;i++)
	{
		cout<<*p<<endl;
		p++;
	}
	return 0;
}
	
